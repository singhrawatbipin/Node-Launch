CREATE TABLE `ums_data`.`user` (
    `id` INT NOT NULL,
    `first_name` VARCHAR(20) NOT NULL,
    `last_name` VARCHAR(20) NOT NULL,
    `email` VARCHAR(20) NOT NULL,
    `phone` VARCHAR(10) NOT NULL,
    `comments` TEXT NOT NULL,
    `status` VARCHAR NOT NULL DEFAULT 'active',
    PRIMARY KEY (`id`)
) ENGINE = InnoDB;